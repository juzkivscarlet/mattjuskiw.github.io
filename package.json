{
	"name": "portfolio",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"homepage": "https://mattjuskiw.github.io/",
	"dependencies": {
		"bootstrap": "^4.4.1",
		"express": "^4.17.1",
		"if-env": "^1.0.4",
		"path": "^0.12.7",
		"react-bootstrap": "^1.0.0-beta.16",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"concurrently": "^5.0.2",
		"gh-pages": "^2.2.0",
		"nodemon": "^2.0.2"
	},
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"start:prod": "node server.js",
		"start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
		"client": "cd client && npm run start",
		"seed": "node scripts/seedDB.js",
		"install": "cd client && npm install",
		"build": "cd client && npm run build",
		"heroku-postbuild": "npm run build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mattjuskiw/PortfolioReact.git"
	},
	"author": "Matt Juskiw",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/mattjuskiw/PortfolioReact/issues"
	},
	"homepage": "https://github.com/mattjuskiw/PortfolioReact#readme"
}
